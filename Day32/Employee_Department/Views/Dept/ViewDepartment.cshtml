@model IEnumerable<Employee_Department.Models.Department>

@{
    ViewBag.Title = "ViewDepartment";
}

<h2>ViewDepartment</h2>

<p>
    @Html.ActionLink("Create New", "Add","Dept")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.DeptName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.DeptName)
        </td>
        <td>
            @Html.ActionLink("EmployeeDetails", "ViewEmployee","Employee") |

            @Html.ActionLink("Edit", "Edit", new { id = item.DId }) |
            @Html.ActionLink("Details", "Details", new { id = item.DId }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.DId })
        </td>
    </tr>
}

</table>
