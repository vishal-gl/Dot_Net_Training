@model IEnumerable<Practise_MVC.Models.Student>

@{
    ViewBag.Title = "ViewStudent";
}

<h2>ViewStudent</h2>

<p>
    @Html.ActionLink("Register", "Register","Student")
</p>
<table class="table">
    <tr>
       
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
       
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        
        <td>
            @Html.ActionLink("Edit", "SEdit", new { id=item.SId }) |
            @Html.ActionLink("Details", "SDetails", new { id=item.SId }) |
            @Html.ActionLink("Delete", "SDelete", new { id=item.SId })
        </td>
    </tr>
}

</table>
